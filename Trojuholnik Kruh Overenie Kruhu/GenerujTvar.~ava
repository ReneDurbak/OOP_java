public class GenerujTvar{
  public static void main(String arg[]){
    
    //Obdlznik() - specialna metoda inplicitny konstruktor
    Obdlznik obdA = new Obdlznik(5.0F, 7.0F,"RED");
    obdA.info();
    
    Obdlznik obdB = new Obdlznik(10.0F, 15.0F,"BLUE");
    obdB.info();
    
    Obdlznik obdC = new Obdlznik(20.5F, "GREEN");
    obdC.info();
    
    Obdlznik obdD = new Obdlznik();
    obdD.info();
    
    
    
    System.out.println("\n\n\n\n\nTROJUHOLNIKY");
    
    Trojuholnik troA = new Trojuholnik();
    troA.info();
    
    Trojuholnik troB = new Trojuholnik(4.0F, 5.0F, 6.0F, "BLUE");
    troB.info();
    
    Trojuholnik troC = new Trojuholnik(3.5F, 8.5F, 5.5F);
    troC.info();
    
    Trojuholnik troD = new Trojuholnik(9.9F, "YELLOW");
    troD.info();
    
    Trojuholnik troE = new Trojuholnik(3.3F, 4.4F, "PINK");
    troE.info();
    
    
    
    System.out.println("\n\n\n\n\nKRUHY");
    
    Kruh kruA = new Kruh();
    kruA.info();
    
    Kruh kruB = new Kruh(5.5F);
    kruB.info();
    
    Kruh kruC = new Kruh(4.5F, "ORANGE");
    kruC.info();
  }

  
}